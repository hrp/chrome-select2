{
  "name": "Select2 for Chrome",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "Improve select form boxes in Chrome with Select2 library",
  "icons": {
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "js/lib/require.js",
        "js/lib/jquery.js",
        "js/lib/underscore.js",
        "js/lib/select2.js",
        "js/requireContent.js",
        "js/requireConfig.js",
        "js/content.js"
      ],
      "css": [
        "css/select2.css"
      ]
    }
  ],
  "permissions": [
  ],
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "images/*"
  ]
}
